if(null != idxs && area && (area.dims ||area.meas)  
&& 
   _currMapDict && _currMapDict.data && _currMapDict.data.metadata && _currMapDict.data.metadata.contexts )
{
var dbid = null
if(area.meas)
{
if(!area.meas.fdbid)
