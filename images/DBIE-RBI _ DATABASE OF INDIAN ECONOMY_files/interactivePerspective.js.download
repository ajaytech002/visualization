var len=words.length
for (var i = 0; i < len; i++) {
if (s.length == 36 )
break
var w = words[i]
s[s.length] = words[i]
}
var pointPoint = ''
if(text.length > s.length)
pointPoint = " ..."
text = s.join('') + pointPoint
}
return text
