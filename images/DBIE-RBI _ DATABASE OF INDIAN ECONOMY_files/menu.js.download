function MenuWidget_justInTimeInit()
{
var o=this
o.layer=getLayer(o.id)
if (o.layer==null)
{
targetApp(o.getHTML())
o.layer=getLayer(o.id)
}
o.layer.setAttribute("role", "menu")
o.layer._widget=o.widx
o.css=o.layer.style
o.layer.oncontextmenu=MenuWidget_ContextMenu
o.endLink=newWidget("endLink_"+o.id)
o.endLink.init()
o.startLink=newWidget("startLink_"+o.id)
o.startLink.init()
var items=o.items, len=o.items.length
for (var i=0; i < len; i++) {
items[i].init()
}
}
