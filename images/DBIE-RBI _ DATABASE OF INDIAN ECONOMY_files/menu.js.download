{
var temppos = curpos + nLastColorPerLine;
if(ypos==1 && temppos>idxLastColor && temppos<=idxLastColor+lenLastUsed)
{
curpos = temppos;
}
else
{
curpos += (ypos*nColorPerLine);
if(curpos>idxLastColor && ypos==1)
{
curpos=idxLastColor+1; //search for the LastUsedColor below
}
}
if(curpos<0)
curpos=0;
}
else if(menuItemType == _isLastUsedColor)
{
curpos+=ypos*nLastColorPerLine;
//curpos=(ypos==1?idxMoreColor:idxLastColor);
if(curpos>lenItems && ypos==1)
curpos = idxMoreColor;
}
else if(menuItemType == _isNotColor)
{
curpos+=ypos;
}
}
if(curpos<0) curpos=lenItems-1;
if(curpos>=lenItems) curpos=0;
item=items[curpos];
if(item.text=="") //_isLastUsedColor not filled
{
if(xpos==-1 || ypos==-1)
{
while(item.text=="")
{
curpos -=1; 
item=items[curpos];
}
}
else
{
curpos = idxMoreColor;
}
}
item=items[curpos];
if(item)
{
item.focus();
if(curpos != idxMoreColor)
item.check(true);
}
}
