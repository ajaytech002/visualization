function MenuWidget_getMenuTBody()
{
return this.layer.childNodes[0].childNodes[1].childNodes[0].childNodes[0].childNodes[0]
}
function MenuWidget_removeByID(id)
{
var o=this;
var item = o.getItemByID(id);
if(item)
{
//remove in items
arrayRemove(o,'items',item.menuIndex);
o.updateIndex();
//remove in html
if (o.layer==null)
return
var tbody=o.getMenuTBody()
tbody.deleteRow(item.menuIndex);
}
}
function MenuWidget_remove(index)
{
var o=this;
if(index != null)
{
arrayRemove(o,'items',index);
}
else //null = remove all
{
o.items.length = 0;
}
o.updateIndex();
//remove in html
if (o.layer==null)
{
//
o.layer = getLayer(o.id);
if (o.layer==null)
return
}
if (index != null)
{
var tbody=o.getMenuTBody()
tbody.deleteRow(index);
}
else
{
    //if (o.layer.outerHTML)
    //    alert("before:"+o.layer.outerHTML);
    if (_ie)
    {
        var top=getLayer('scrTop_'+o.id);
        var bottom=getLayer('scrBottom_'+o.id);
        var menuDiv=o.menuDiv?o.menuDiv:getLayer("menuDiv_"+o.id);
        if (top)
            top.outerHTML="  ";
            if (bottom)
                bottom.outerHTML="  ";    
            if (menuDiv)
                menuDiv.outerHTML="  ";                          
    }
    //if (o.layer.outerHTML)
    //    alert("after:"+o.layer.outerHTML);
        o.layer.parentNode.removeChild(o.layer);
        o.layer=null
}
}
