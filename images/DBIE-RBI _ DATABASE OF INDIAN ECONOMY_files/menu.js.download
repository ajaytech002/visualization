x=Math.max(0,x-4-w)
if (y2>winHeight())
y=Math.max(0,y-4-h)
css.left=""+x+"px"
css.top=""+y+"px"
css.visibility="visible"
// Show and place menu shadow
if (o.iframeLyr==null)
{
o.iframeLyr=getDynamicBGIFrameLayer()
o.iframeCss=o.iframeLyr.style
}
iCss=o.iframeCss
iCss.left=""+x+"px"
iCss.top=""+y+"px"
iCss.width=""+w+"px"
iCss.height=""+h+"px"
iCss.zIndex=o.zIndex-1
iCss.display='block'
