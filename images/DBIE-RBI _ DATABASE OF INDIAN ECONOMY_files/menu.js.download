,getHTMLPart : function(part)
{
var o=this
switch(part)
{
case 0: // the icon on the left
{
var im=null;
if (o.isCheck && (o.icon == null)) 
{
im = '<div id="'+(o.par.id + '_item_icon_' + o.id)+'" class="'+(o.checked?"menuIconCheckTick":"menuIconCheckNoTick")+'" title="'+((o.checked ? _menuCheckLab : ""))+'" />'
}
else 
{
var tooltip = o.alt ? o.alt:(o.text?o.text:'');
im = o.icon ? simpleImgOffset(o.icon, o.iconWidth, o.iconHeight, o.disabled ? o.disDx : o.dx, o.disabled ? o.disDy : o.dy, (o.par.id + '_item_icon_' + o.id), null, tooltip) : (getSpace(o.iconWidth, o.iconHeight))
}
if (o.isCheck)
{
    var className=' class="'+(o.checked?o.par.menuIconCheckClass:o.par.menuIconClass)+'"'     
im='<div id="'+o.par.id+'_item_check_'+o.id+'" '+className+' align="center" style="width:'+o.iconWidth+'px;height:'+o.iconHeight+'px;padding:2px">'+im+'</div>';
}
var width=o.leftPartWidth-3;
var s='<table width="100%" style="height:'+_mitemH+'" border="0" cellspacing="0" cellpadding="0"><tr style="height:'+_mitemH+'px">'; 
s+='<td valign="top" style="width:3px"><div style="width:3px"></div></td>';
s+='<td valign="middle" width='+width+'><div style="width:'+width+'px">'+im+'</div></td>'
s+='</tr></table>';
return s
}
case 1: // the text
{   
var tabIndexInfo=o.disabled?' tabIndex="-1"':' tabIndex="0"'; 
return '<span id="'+(o.par.id+'_span_text_'+o.id)+'"'+tabIndexInfo+'>'+convStr(o.text)+'</span>';
        }
case 2:
{
var im=simpleImgOffset(_skin+_menusImgFile,16,16, o.sub?(o.disabled?7:0):0, (o.sub?(o.disabled?(_rtl?528:82):(_rtl?514:64)):0), o.par.id+
'_item_arrow_'+o.id, null, null, null,"right");
var s='<table width="100%" style="height:'+_mitemH+'" border="0" cellspacing="0" cellpadding="0"><tr align=' + (_rtl?"left":"right") + ' style="height:'+_mitemH+'px">'; 
s+='<td valign="middle">'+im+'</td>'
s+='<td style="width:3px" valign="top"><div style="width:3px"></div></td>';
s+='</tr></table>';
return s;
}
case 3: // The separator
return '<table width="100%" height="3" cellpadding="0" cellspacing="0" border="0" style="'+backImgOffset(_skin+_menusImgFile,0,80)+';"><tbody><tr><td></td></tr></tbody></table>'
case 4:// the accelerator
{
var s='<table width="100%" border="0" cellspacing="0" cellpadding="0"><tr align="right" style="height:'+_mitemH+'px">'; 
s+='<td valign="middle" class="'+(o.disabled?o.par.menuTextPartClassDis:o.par.menuTextPartClass)+'" id="'+(o.par.id+'_accel_'+o.id)+'">'+(_mac?o.accel:convStr(o.accel))+'</td>'
s+='<td class="menuRightPart" style="width:3px" valign="top"><div style="width:3px"></div></td>';
s+='</tr></table>';
