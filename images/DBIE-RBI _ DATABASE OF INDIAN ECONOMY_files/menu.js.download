 }
 else if (realPart==1)
 ce.className=o.disabled?o.par.menuTextPartClassDis:(inv?o.par.menuTextPartClassSel:o.par.menuTextPartClass)
 else if (o.accel && (realPart==2)) 
 {
 var acc=ce.childNodes[0].childNodes[0].childNodes[0].childNodes[0];
 acc.className=o.disabled?o.par.menuTextPartClassDis:(inv?o.par.menuTextPartClassSel:o.par.menuTextPartClass)
 acc=ce.childNodes[0].childNodes[0].childNodes[0].childNodes[1];
 acc.className=inv?(_rtl?'menuLeftMostSel':'menuRightMostSel'):o.par.menuRightPartClass
 break
 }
 else
 {
 ce.className=inv?o.par.menuRightPartClassSel:o.par.menuRightPartClass
 var rightMost=ce.childNodes[0].childNodes[0].childNodes[0].childNodes[1];
 rightMost.className=inv?(_rtl?'menuLeftMostSel':'menuRightMostSel'):o.par.menuRightPartClass
 }
 realPart++
 }
