var idx = (o.idxFocusItem!=null?o.idxFocusItem:0);
var lenLastUsed =  o.lastUsedColorsAr?o.lastUsedColorsAr.length:0;
for(var i=0; i<len; i++)
{
var item=items[i]
if (item.checked)
{
idx =i;
item.check(false);
break;
}
}
MenuColorWidget_moveCursor(idx,items,lenLastUsed, (key==37?-1:(key==39?1:null)),(key==38?-1:(key==40?1:null)))
}
else if(key==13)//enter
{
eventCancelBubble(e);//be careful ! usefull for dialog box close by Enter keypressed
}
}
