 else
 {
 css.display="block"
 }
 */
if (!_dtd4) {
y -= 2
x -= 2
}
o.nextSub = -1
o.showSub()
o.focus()
});
}
else {
/*
 if (null != o.appearEffect) {
 o.appearEffect.cancel()
 o.css.display="block"
