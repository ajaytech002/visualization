*/function isParentHidden(elt)
{
    if (!elt)
        return false;
    else
    if (elt.isHide)
        return true;
    else     
        return isParentHidden(elt.parent);      
}
function evalContextPalettes(elts)
{
var len = elts ? elts.length : 0
var type = isCompositeSelection(elts)
if (len == 1) 
{
var elt = elts[0]
var table = isTableBlock(elt)
var graph = isGraphBlock(elt)
var tableCell = isTableCell(elt)
var freeCell = isFreeCell(elt) || isCellInSection(elt);
var section = (elt.className == "section")
        var isHide= false
if (!isStructure) {
isHide = (typeof(elt.isHide)!="undefined")?elt.isHide:false;
        if (!isHide)
        {
            isHide=isParentHidden(elt.parent);
        }
         }
_currWorkbench.getAction("groups.onTableSelected").setDisplay(tableCell && !isHide);
_currWorkbench.getAction("groups.onFreeCell").setDisplay(freeCell && !isHide);
_currWorkbench.getAction("groups.ReportElementOnChart").setDisplay(graph && !isHide);
_currWorkbench.getAction("groups.formatChartStyle").setDisplay(graph && !isHide);
_currWorkbench.getAction("groups.ReportElementOnTable").setDisplay((table||tableCell) && !isHide);
_currWorkbench.getAction("groups.ReportElementOnSection").setDisplay(section && !isHide);
} 
else 
{
_currWorkbench.getAction("groups.onTableSelected").setDisplay(false);
_currWorkbench.getAction("groups.onFreeCell").setDisplay(false);
_currWorkbench.getAction("groups.ReportElementOnChart").setDisplay(false);
_currWorkbench.getAction("groups.formatChartStyle").setDisplay(false);
_currWorkbench.getAction("groups.ReportElementOnTable").setDisplay(type.isSameTable);
_currWorkbench.getAction("groups.ReportElementOnSection").setDisplay(false);
}
}
