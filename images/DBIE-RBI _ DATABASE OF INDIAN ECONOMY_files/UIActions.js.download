function fillAllActions(workbench)
{
apply_actions_customization();
workbench.fillActions(webi.actions);
delete webi.actions;
}
function apply_actions_customization()
{
if (typeof(apply_customization_shortcut) != "undefined")
{
apply_customization_shortcut(webi.actions);
}
if (typeof(apply_customization_feature) != "undefined")
{
apply_customization_feature(webi.actions);
}
}
