				$('[name='+iframe.name+']').load(function(){var iframe = frames[1].document;		var adhocTable = iframe.getElementsByTagName("table")[0];		if(adhocTable!=null || adhocTable!=undefined){		adhocTable.style.display = 'none';				var noOfRows = adhocTable.rows.length;		for(var i=0; i<noOfRows; i++)		{			adhocTable.rows[i].cells[5].style.display="none";			adhocTable.rows[i].cells[6].style.display="none";			adhocTable.rows[i].cells[7].style.display="none";			adhocTable.rows[i].cells[8].style.display="none";		}		for(var i=0; i<noOfRows; i++)	{		if($.trim($(adhocTable.rows[i].cells[2]).text())=="")		{			$(adhocTable.rows[i]).hide();		}	}	adhocTable.style.display = 'block';}});
			}
		}
}

function reportsLoad(hash) {
	// hash doesn't contain the first # character.
	if(hash) {
		// restore ajax loaded state
		$.ajax({
			url: 'statistics.rbi',
			type: 'POST',
			/* data:{
				action:'FR',
				d:hash.split('_')[0],
				sd:hash.split('_')[1]
			} */
			data:{
				action:'FR',
				p:(hash=='FR'?'':hash)
			},
			success: function(response, obj)
			{
				try{
					var JSONResponse = $.parseJSON(response);
					$("#breadCrumb").html(JSONResponse.bread_crumb);
					$("#body_content").html(JSONResponse.body_content);
					if(JSONResponse.success){
						var mypets=new ddajaxtabs("pettabs", "petsdivcontainer");
			 			mypets.setpersist(false);
			 			mypets.setselectedClassTarget("link");
			 			mypets.onajaxpageload(resizeIframe);
			 			mypets.init();
			 			//$('a[site=adhoc]').click(function(){
				 			//setTimeout(frames[1].onload = function(){ var iframe = frames[1].document;		var adhocTable = iframe.getElementsByTagName("table")[0];		if(adhocTable!=null || adhocTable!=undefined){		adhocTable.style.display = 'none';				var noOfRows = adhocTable.rows.length;		for(var i=0; i<noOfRows; i++)		{			adhocTable.rows[i].cells[5].style.display="none";			adhocTable.rows[i].cells[6].style.display="none";			adhocTable.rows[i].cells[7].style.display="none";			adhocTable.rows[i].cells[8].style.display="none";		}		for(var i=0; i<noOfRows; i++)	{		if($.trim($(adhocTable.rows[i].cells[2]).text())=="")		{			$(adhocTable.rows[i]).hide();		}	}	adhocTable.style.display = 'block';}},1000);
				 		//});
					}
				}
				catch(e)
				{
					window.location.href = window.location.href;
				}
			}
			
		});
	} else {
		// start page
		$("#breadCrumb").html(breadCrumbStatsHTML);
		$("#body_content").hide();
		$("#body_content").empty().html('<div class="warningbox" > Invalid Page Requested </div>').slideDown();
	}
}


